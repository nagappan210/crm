generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model candidate {
  id                      Int      @id @default(autoincrement())
  enquiry_raised_date     DateTime @db.DateTime(0)
  enquiry_answered_date   DateTime @db.DateTime(0)
  name                    String?  @db.VarChar(255)
  phone_number            Int
  alternative_phonenumber Int      @default(0) @db.MediumInt
  email                   String?  @db.VarChar(255)
  location                String?  @db.Text
  role                    String?  @db.VarChar(255)
  experience              String?  @db.VarChar(255)
  months                  String?  @db.VarChar(255)
  degree                  String?  @db.VarChar(255)
  added_by                String?  @db.VarChar(255)
  created_at              DateTime @default(now()) @db.DateTime(0)
  updated_at              DateTime @default(now()) @db.DateTime(0)
  s_delete                Int      @default(0) @db.TinyInt
}

model client {
  id                      Int      @id @default(autoincrement())
  name                    String   @db.Text
  phone_number            Int      @db.MediumInt
  alternative_phonenumber Int      @default(0) @db.MediumInt
  email                   String?  @db.VarChar(255)
  location                String?  @db.VarChar(255)
  business_type           String?  @db.VarChar(255)
  lead                    String?  @db.VarChar(255)
  company_name            String   @db.VarChar(255)
  added_by                String   @db.VarChar(255)
  created_at              DateTime @default(now()) @db.DateTime(0)
  updated_at              DateTime @default(now()) @db.DateTime(0)
  s_delete                Int      @default(0) @db.TinyInt
}

model payments {
  id                Int      @id @default(autoincrement())
  paid_date         DateTime @db.DateTime(0)
  project_id        Int
  total_amount      Int
  paid              Int
  currency_type     Int
  amount_paid       Int
  balance_amount    Int
  total_amount_paid Int
  method            Int
  status            Int
  transaction_id    Int
  image             Int
  created_at        DateTime @default(now()) @db.DateTime(0)
  updated_at        DateTime @default(now()) @db.DateTime(0)
}

model qutation {
  id           Int      @id @default(autoincrement())
  client_id    Int
  enquire_data DateTime @db.DateTime(0)
  company_name String   @db.VarChar(255)
  phone        Int
  email        String   @db.VarChar(255)
  address      String   @db.VarChar(500)
  description  String   @db.MediumText
  tax          Float    @db.Float
  amount       Float
  updated_at   DateTime @default(now()) @db.DateTime(0)
  created_at   DateTime @default(now()) @db.DateTime(0)
  s_delete     Int      @default(0) @db.TinyInt
}

model service {
  id         Int      @id @default(autoincrement())
  name       String?  @db.VarChar(255)
  created_at DateTime @default(now()) @db.DateTime(0)
  updated_at DateTime @default(now()) @db.DateTime(0)
  s_delete   Int      @default(0) @db.TinyInt
}

model business {
  id         Int      @id @default(autoincrement())
  name       String?  @db.Text
  created_at DateTime @default(now()) @db.DateTime(0)
  updated_at DateTime @default(now()) @db.DateTime(0)
  s_delete   Int      @default(0) @db.TinyInt
}

model department {
  id         Int      @id @default(autoincrement())
  name       String?  @db.Text
  created_at DateTime @default(now()) @db.DateTime(0)
  updated_at DateTime @default(now()) @db.DateTime(0)
  s_delete   Int      @default(0) @db.TinyInt
}

model spam {
  id                    Int      @id @default(autoincrement())
  email                 String?  @db.VarChar(255)
  phone_number          Int      @default(0)
  enquiry_raised_date   DateTime @db.DateTime(0)
  enquiry_answered_date DateTime @db.DateTime(0)
  name                  String   @db.VarChar(255)
  location              String   @db.VarChar(255)
  leads                 String   @db.VarChar(255)
  remarks               String?  @db.VarChar(255)
  added_by              String?  @db.VarChar(255)
  created_at            DateTime @default(now()) @db.DateTime(0)
  updated_at            DateTime @default(now()) @db.DateTime(0)
  s_delete              Int      @default(0) @db.TinyInt
}

model user {
  id         Int      @id @default(autoincrement())
  name       String?  @unique(map: "name") @db.VarChar(255)
  password   String?  @db.VarChar(255)
  roll       String?  @db.VarChar(255)
  created_at DateTime @default(now()) @db.DateTime(0)
}

model candidate_roll {
  id         Int      @id @default(autoincrement())
  name       String   @db.VarChar(255)
  created_at DateTime @default(now()) @db.DateTime(0)
  updated_at DateTime @default(now()) @db.DateTime(0)
  s_delete   Int      @default(0) @db.TinyInt
}

model follow_up_files {
  id          Int      @id @default(autoincrement())
  enquire_id  Int
  file_type   Int
  file_path   String   @db.VarChar(255)
  followup_id Int
  project_id  Int      @default(0)
  created_at  DateTime @default(now()) @db.DateTime(0)
  updated_at  DateTime @default(now()) @db.DateTime(0)
}

model payment_files {
  id             Int      @id @default(autoincrement())
  payment_id     Int
  transcation_id String?  @db.VarChar(255)
  path           String?  @db.VarChar(255)
  created_at     DateTime @default(now()) @db.DateTime(0)
  updated_at     DateTime @default(now()) @db.DateTime(0)
}

model enquire {
  id                    Int      @id @default(autoincrement())
  enquiry_raised_date   DateTime @db.DateTime(0)
  enquiry_answered_date DateTime @db.DateTime(0)
  client_id             Int
  service_id            String   @db.VarChar(255)
  created_at            DateTime @default(now()) @db.DateTime(0)
  remarks               String   @db.VarChar(1000)
  dead_line             String   @db.VarChar(100)
  added_by              String   @db.VarChar(255)
  updated_at            DateTime @default(now()) @db.DateTime(0)
}

model enquiry_details_file {
  id         Int      @id @default(autoincrement())
  enquire_id Int
  file_type  Int
  file_path  String   @db.VarChar(255)
  notes      String?  @db.VarChar(1000)
  created_at DateTime @default(now()) @db.DateTime(0)
  updated_at DateTime @default(now()) @db.DateTime(0)
}

model follow_up {
  id          Int      @id @default(autoincrement())
  service_id  Int
  project_id  Int
  intrested   Int
  enquirey_id Int
  remark      String   @db.VarChar(1000)
  created_at  DateTime @default(now()) @db.DateTime(0)
  updated_at  DateTime @default(now()) @db.DateTime(0)
}

model project {
  id         Int      @id @default(autoincrement())
  service_id Int
  enquiry_id Int
  client_id  Int
  created_at DateTime @default(now()) @db.DateTime(0)
  updated_at DateTime @default(now()) @db.DateTime(0)
}

model project_details_file {
  id         Int      @id @default(autoincrement())
  enquire_id Int
  file_type  Int
  file_path  String   @db.VarChar(255)
  notes      String?  @db.VarChar(1000)
  created_at DateTime @default(now()) @db.DateTime(0)
  updated_at DateTime @default(now()) @db.DateTime(0)
}

model requirements_answer {
  id          Int      @id @default(autoincrement())
  question_id Int
  enquire_id  Int
  answer      Int
  project_id  Int
  service_id  Int
  created_at  DateTime @default(now()) @db.DateTime(0)
  updated_at  DateTime @default(now()) @db.DateTime(0)
}

model requirements_questions {
  id         Int      @id @default(autoincrement())
  type       Int
  question   String   @db.VarChar(255)
  service_id Int
  project_id Int
  enquire_id Int
  s_delete   Int      @default(0) @db.TinyInt
  created_at DateTime @default(now()) @db.DateTime(0)
  updated_at DateTime @default(now()) @db.DateTime(0)
}
